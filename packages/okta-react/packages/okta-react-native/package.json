    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|react-navigation|@react-navigation/.*|@unimodules/.*|sentry-expo|native-base)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okta/okta-oidc-js.git"
  },
  "license": "Apache-2.0",
