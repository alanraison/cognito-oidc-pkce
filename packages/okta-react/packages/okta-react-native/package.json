{
  "name": "@okta/okta-react-native",
  "version": "0.1.2",
  "description": "React Native support for OpenID Connect",
  "main": "src/token-client.js",
  "scripts": {
    "test": "yarn test:unit",
    "test:unit": "jest --coverage --verbose --testPathPattern __tests__/.*.test.js"
  },
  "jest": {
    "browser": true,
    "preset": "jest-expo",
    "automock": false,
    "setupFiles": [
