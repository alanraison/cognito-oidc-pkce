  ],
  "dependencies": {
    "@okta/configuration-validation": "^0.3.0",
    "b64u-lite": "^1.0.1",
    "dlv": "^1.1.2",
    "expo-constants": "^4.0.0",
    "expo-secure-store": "^4.0.0",
    "expo-web-browser": "^4.0.0",
    "isomorphic-webcrypto": "^1.6.1",
    "jwt-lite": "^1.1.2",
    "str2buf": "^1.3.0"
  },
  "peerDependencies": {
    "react-native": "*",
    "react-native-securerandom": "^0.1.1",
    "react-native-unimodules": "0.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-jest": "^24.8.0",
    "expo-react-native-adapter": "^3.0.1",
    "jest": "^24.1.0",
    "jest-expo": "^32.0.1",
    "jest-fetch-mock": "^2.1.2",
    "metro-react-native-babel-preset": "0.52.0",
    "react": "*",
    "react-native": "0.58.6",
    "react-native-unimodules": "0.3.1",
    "react-native-gesture-handler": "^1.2.2"
  }
